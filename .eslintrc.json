{
    "env": {
        "es2021": true,
        "node": true
    },
    "plugins": ["jest", "import"],
    "extends": ["plugin:jest/recommended", "standard"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "max-params": ["error", 3],
      "semi": ["error", "never"],
      "camelcase": "error",
      "curly": "error",
      "jest/valid-describe": "off"
    }
}
